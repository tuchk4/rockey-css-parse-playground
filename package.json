{
  "name": "rockey-css-parse-repl",
  "homepage": "https://tuchk4.github.io/rockey-css-parse-repl/",
  "description": "REPL for rockey-css-parse",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "cssbeautify": "^0.3.1",
    "gh-pages": "^1.0.0",
    "highlight.js": "^9.11.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "prettier": "^1.3.1",
    "react": "^15.5.4",
    "react-codemirror": "^1.0.0",
    "react-dom": "^15.5.4",
    "react-json-tree": "^0.10.9",
    "react-layout-components": "^3.0.4",
    "react-tabs": "^1.0.0",
    "rockey": "0.0.15-1",
    "rockey-css-parse": "0.0.15-1"
  },
  "devDependencies": {
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/*/*.js'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  }
}
